{
  "name": "Badger",
  "manifest_version": 3,
  "version": "0.0.1",
  "description": "__MSG_manifestDescription__",
  "author": "bigcookie",
  "homepage_url": "https://github.com/biggestcookie/Badger",
  "icons": {
    "16": "assets/icons/16.png",
    "48": "assets/icons/48.png",
    "128": "assets/icons/128.png"
  },
  "permissions": [
    "alarms",
    "notifications",
    "storage"
  ],
  "default_locale": "en",
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "action": {
    "default_popup": "pages/popup/index.html",
    "default_title": "Badger",
    "default_icon": {
      "19": "assets/icons/19.png",
      "38": "assets/icons/38.png"
    }
  },
  "options_ui": {
    "page": "pages/options/index.html",
    "open_in_tab": true,
    "chrome_style": false
  },
  "incognito": "spanning"
}
