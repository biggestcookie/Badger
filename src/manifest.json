{
  "name": "Badger",
  "description": "__MSG_manifestDescription__",
  "author": "bigcookie",
  "homepage_url": "https://github.com/biggestcookie/Badger",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "permissions": [
    "alarms",
    "notifications",
    "storage"
  ],
  "default_locale": "en",
  "manifest_version": 2,
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Badger",
    "default_icon": {
      "19": "icons/19.png",
      "38": "icons/38.png"
    }
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true,
    "chrome_style": false
  },
  "incognito": "spanning"
}
